                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    bottomPadding="30"
                    color="style-bg-gray-3"
                    leftPadding="30"
                    rightPadding="30"
                    textOnly="&lt;h3>IN THIS ARTICLE&lt;/h3>&#xa;&lt;ul>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>HPE InfoSight automatically predicts and resolves 86 percent of issues before a problem is identified&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>AI-driven operations enable customers to gain higher efficiency and reliability with smarter, easy-to-manage infrastructures&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                    topPadding="30"/>
                <horizontalgrayruleli
                    jcr:created="{Date}2018-06-18T09:16:54.243-07:00"
