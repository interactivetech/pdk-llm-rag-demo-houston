                    topMargin="30"
                    videoSource="video"
                    videoType="youtube"/>
                <textonlycomponent_b7
                    jcr:created="{Date}2018-10-18T12:49:12.473Z"
                    jcr:createdBy="camelia.manoiu@hpe.com"
                    jcr:lastModified="{Date}2018-10-31T10:23:39.827Z"
                    jcr:lastModifiedBy="silviu.gavriluta@hpe.com"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    textOnly="&lt;p>&lt;span class=&quot;body-copy-large&quot;>&amp;quot;The organizations that can turn data into knowledge, knowledge into insights and insights into outcomes will be the winners&amp;quot;, said Jim Jackson, Chief Marketing Officer, HPE. &amp;quot;Our sponsorship of Mercedes-AMG Petronas Motorsport is all about bringing the power of the world's best technology and services to a team with a history of innovation to fuel performance at the highest possible level.&amp;quot;&lt;/span>&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>About Hewlett Packard Enterprise&lt;/b>&lt;/span>&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>Hewlett Packard Enterprise is a global technology leader focused on developing intelligent solutions that allow customers to capture, analyze and act upon data seamlessly from edge to cloud. HPE enables customers to accelerate business outcomes by driving new business models, creating new customer and employee experiences, and increasing operational efficiency today and into the future.&lt;/span>&lt;/p>&#xa;&lt;p>&amp;nbsp;&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>Contacts:&lt;/b>&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;i>&lt;b>Bradley Lord&lt;/b>&lt;/i>&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>Mercedes-AMG Motorsport Communications Director&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;a href=&quot;mailto:blord@mercedesamgf1.com&quot; target=&quot;_blank&quot; rel=&quot;noopener&quot;>&lt;b>blord@mercedesamgf1.com&lt;/b>&lt;/a>&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>+44 (0) 7785 682 893&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&amp;nbsp;&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;i>&lt;b>Lindsey Berryhill&lt;/b>&lt;/i>&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>HPE&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;a href=&quot;mailto:Lindsey.berryhill@hpe.com&quot; target=&quot;_blank&quot; rel=&quot;noopener&quot;>&lt;b>Lindsey.berryhill@hpe.com&lt;/b>&lt;/a>&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>+1 650 518 6131&lt;/span>&lt;/p>&#xa;"/>
