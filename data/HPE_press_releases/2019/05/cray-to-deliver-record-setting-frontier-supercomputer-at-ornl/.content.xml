                <textonlycomponent
                    jcr:created="{Date}2020-09-10T17:10:16.377Z"
                    jcr:createdBy="christine.seales@hpe.com"
                    jcr:lastModified="{Date}2020-09-10T17:10:30.786Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
