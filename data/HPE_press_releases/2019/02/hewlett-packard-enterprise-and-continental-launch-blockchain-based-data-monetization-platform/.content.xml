                <textonlycomponent_cd
                    jcr:created="{Date}2018-06-18T09:12:49.742-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2019-02-22T11:44:37.889Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    bottomPadding="30"
                    color="style-bg-gray-3"
                    leftPadding="30"
                    rightPadding="30"
                    textOnly="&lt;h3>IN THIS ARTICLE&lt;/h3>&#xa;&lt;ul>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>Based on blockchain technology, HPE and Continental’s platform provides data sovereignty, security, transparency, and efficiency to overcome the barriers of sharing vehicle data&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>The platform enables digital services that improve driver safety and convenience, while helping car manufacturers to monetize their data and differentiate their brands&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                    topPadding="30"/>
                <horizontalgrayruleli
                    jcr:created="{Date}2018-06-18T09:16:54.243-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2018-06-18T09:18:45.987-07:00"
                    jcr:lastModifiedBy="velazquez@hpe.com"
