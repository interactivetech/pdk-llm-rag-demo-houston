                    leftPadding="30"
                    rightPadding="30"
                    textOnly="&lt;h3>IN THIS ARTICLE&lt;/h3>&#xa;&lt;ul>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>HPE ML Ops delivers the industry’s most comprehensive operationalization for machine learning&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>New container-based software solution accelerates AI innovation, unlocking new business value&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                    topPadding="30"/>
                <horizontalgrayruleli
                    jcr:created="{Date}2018-06-18T09:16:54.243-07:00"
