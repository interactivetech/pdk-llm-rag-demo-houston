                    bottomPadding="30"
                    color="style-bg-gray-3"
                    leftPadding="30"
                    rightPadding="30"
                    textOnly="&lt;h3>IN THIS ARTICLE&lt;/h3>&#xa;&lt;ul>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>HPE announces BlueShore Financial as first global HPE Primera customer&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>BlueShore Financial turns to HPE Primera to foster innovation and deliver superior products and services to clients&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>HPE Primera is now available globally&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                    topPadding="30"/>
                <horizontalgrayruleli
