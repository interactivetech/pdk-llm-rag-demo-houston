                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    designAccentColor="gray"
                    textIsRich="true"
                    textOnly="&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>Availability&lt;/b>&lt;/span>&lt;/p>&#xd;&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>HPE Primera will be orderable in August 2019.&lt;/span>&lt;/p>&#xd;&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>Additional Resources&lt;/b>&lt;/span>&lt;/p>&#xd;&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>To learn more about HPE Primera, visit: &lt;a href=&quot;https://hpe.com/storage/Primera&quot; target=&quot;_blank&quot;>https://hpe.com/storage/Primera&lt;/a>&lt;/span>&lt;/p>&#xd;&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>IDC &lt;a href=&quot;https://www.hpe.com/us/en/resources/storage/hpe-primera.html&quot; target=&quot;_blank&quot;>white paper&lt;/a>&lt;/span>&lt;/p>&#xd;&#xa;"/>
                <textonlycomponent_fe
                    jcr:created="{Date}2018-06-18T09:28:54.456-07:00"
                    jcr:createdBy="velazquez@hpe.com"
                    jcr:lastModified="{Date}2019-06-15T17:09:19.862Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
                    jcr:primaryType="nt:unstructured"
                    sling:resourceType="hpeweb/components/content/textOnlyComponent"
                    bottomPadding="30"
                    textOnly="&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;b>About Hewlett Packard Enterprise&lt;/b>&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>Hewlett Packard Enterprise is a global technology leader focused on developing intelligent solutions that allow customers to capture, analyze and act upon data seamlessly from edge to cloud. HPE enables customers to accelerate business outcomes by driving new business models, creating new customer and employee experiences, and increasing operational efficiency today and into the future.&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&amp;nbsp;&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;sup>i&lt;/sup>&amp;nbsp;Based on HPE internal testing, compared to previous generation storage arrays.&lt;/span>&lt;/p>&#xa;&lt;p>&lt;span class=&quot;body-copy-large&quot;>&lt;sup>2&lt;/sup>Based on recent survey of IT leaders by IDC&lt;/span>&lt;/p>&#xa;"
                    topPadding="30"/>
