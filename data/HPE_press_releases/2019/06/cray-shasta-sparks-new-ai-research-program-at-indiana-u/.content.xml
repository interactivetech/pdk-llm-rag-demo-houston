                <textonlycomponent
                    jcr:created="{Date}2020-09-10T18:33:59.864Z"
                    jcr:createdBy="christine.seales@hpe.com"
                    jcr:lastModified="{Date}2020-09-10T18:34:17.409Z"
                    jcr:lastModifiedBy="christine.seales@hpe.com"
