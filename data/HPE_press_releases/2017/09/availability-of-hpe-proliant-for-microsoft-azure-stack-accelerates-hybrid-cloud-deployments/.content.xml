                    bottomPadding="30"
                    color="style-bg-gray-3"
                    leftPadding="30"
                    rightPadding="30"
                    textOnly="&lt;h3>IN THIS ARTICLE&lt;/h3>&#xa;&lt;ul>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>Run and manage high performance Microsoft Azure-consistent services on-premises&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>Connect with HPE, Microsoft and third party experts to plan, build and manage hybrid clouds&lt;/span>&lt;/li>&#xa;&lt;li>&lt;span class=&quot;body-copy-large&quot;>Operate consumption-based Microsoft Azure Stack services for simplified billing and lower costs&lt;/span>&lt;/li>&#xa;&lt;/ul>&#xa;"
                    topPadding="30"/>
                <horizontalgrayruleli
                    jcr:created="{Date}2018-06-18T09:16:54.243-07:00"
                    jcr:createdBy="velazquez@hpe.com"
